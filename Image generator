from openai import OpenAI

client = OpenAI()

# Combine your fixed prompt with the content from the response_text
prompt = f"create an image which shows a group of people male and females and from every race, so as inclusive as possible, which shows a group of people who's description matches with the profile specified in the following text file, they should look professional {response_text}"

response = client.images.generate(
  model="dall-e-3",
  prompt=prompt,
  size="1024x1024",
  quality="standard",
  n=1,
)

image_url = response.data[0].url
print(image_url)
